{
  "name": "ruedas-rebeldes",
  "version": "1.0.0",
  "private": true,
  "description": "ruedas-rebeldes",
  "author": "ruedas-rebeldes",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "build-staging": "GATSBY_ACTIVE_ENV=staging gatsby build",
    "build-local": "GATSBY_LOCAL_ENV=development gatsby build",
    "develop": "gatsby develop -p 9000",
    "develop-local": "gatsby develop -H 0.0.0.0 -p 8000",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "serve-staging": "GATSBY_STAGE_ENV=staging gatsby serve -H 0.0.0.0 -p 8000",
    "serve-local": "GATSBY_LOCAL_ENV=development gatsby serve -H 0.0.0.0 -p 8000",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.19.2",
    "axios": "^1.6.2",
    "babel-plugin-styled-components": "^2.1.4",
    "gatsby": "^5.12.4",
    "gatsby-omni-font-loader": "^2.0.2",
    "gatsby-plugin-manifest": "^5.12.1",
    "gatsby-plugin-no-sourcemaps": "^5.12.0",
    "gatsby-plugin-react-helmet": "^6.12.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sass": "^6.12.1",
    "gatsby-plugin-sitemap": "^6.12.1",
    "gatsby-plugin-smoothscroll": "^1.2.0",
    "gatsby-plugin-styled-components": "^6.12.0",
    "gatsby-source-filesystem": "^5.12.0",
    "gatsby-source-graphql": "^5.12.0",
    "node-sass": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-awesome-styled-grid": "^3.1.4",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-responsive-carousel": "^3.2.23",
    "react-responsive-image": "^5.0.0",
    "styled-components": "^6.0.8"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "prettier": "^3.0.3"
  }
}
